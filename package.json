{
  "name": "flock-sendbird-chat",
  "version": "0.0.1",
  "description": "Exportable and compiled React component for Sendbird out-of-the-box UI",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "NODE_OPTIONS=--openssl-legacy-provider nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.25.x",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "dependencies": {
    "@sendbird/uikit-react": "^3.14.6",
    "sendbird-uikit": "^2.7.2"
  }
}
